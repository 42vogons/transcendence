{
	"extends": ["next/core-web-vitals",
	"@rocketseat/eslint-config/next",
	"prettier"
	],
	"rules": {
		"prettier/prettier": ["error", {
			"printWidth": 80,
			"tabWidth": 4,
			"useTabs": true,
			"singleQuote": true,
			"trailingComma": "all",
			"arrowParens": "always",
			"semi": false,
			"endOfLine": "auto"
		}],
		"@typescript-eslint/no-non-null-assertion": "off",
		"@typescript-eslint/no-explicit-any": "off"
	}
}
